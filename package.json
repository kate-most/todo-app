  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "css",
      "scss"
    ],
    "coveragePathIgnorePatterns": ["<rootDir>/public/src/helpers/", "<rootDir>/node_modules/"],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
